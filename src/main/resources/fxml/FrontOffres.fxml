                                  </styleClass>
                              </HBox>
                        <Button fx:id="btnMaps" mnemonicParsing="false" onAction="#showMap" prefHeight="40.0" prefWidth="100.0" styleClass="search-button" stylesheets="@style1.css" text="Maps" textFill="white" />
